{
  "name": "@jupyterlite/pyolite-kernel",
  "version": "0.1.0-alpha.0",
  "description": "JupyterLite - Pyolite Kernel",
  "homepage": "https://github.com/jtpio/jupyterlite",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/",
    "py": "py/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js",
    "py/**/*.whl"
  ],
  "scripts": {
    "build": "yarn run build:lib",
    "build:lib": "tsc -b",
    "build:python": "python -m pip install build && cd py && python -m build -w",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "yarn run clean:lib && yarn run clean:python",
    "clean:lib": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "clean:python": "rimraf py/dist",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlite/kernel": "^0.1.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@jupyterlab/testutils": "^3.1.0-alpha.10",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rimraf": "~3.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~4.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "pyolite": {
    "packages": [
      "py/pyolite"
    ]
  },
  "styleModule": "style/index.js"
}
