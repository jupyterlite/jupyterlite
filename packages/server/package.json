    "@lumino/application": "^1.20.0",
    "@lumino/coreutils": "^1.8.0",
    "mock-socket": "^9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@jupyterlab/testutils": "^3.1.0-alpha.8",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rimraf": "~3.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~4.2.3"
  },
  "publishConfig": {
    "access": "public"
