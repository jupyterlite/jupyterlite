    "@types/serviceworker": "^0.0.56",
    "jest": "^29.6.2",
    "rimraf": "~5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
