{
  "name": "@jupyterlite/iframe-extension",
  "version": "0.1.0-alpha.0",
  "description": "JupyterLite - IFrame Viewer",
  "homepage": "https://github.com/jtpio/jupyterlite",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^3.1.0-alpha.10",
    "@lumino/coreutils": "^1.8.0",
    "@lumino/widgets": "^1.19.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "styleModule": "style/index.js"
}
