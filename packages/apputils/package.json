{
  "name": "@jupyterlite/apputils",
  "version": "0.8.0-alpha.0",
  "description": "JupyterLite - Application Utilities",
  "homepage": "https://github.com/jupyterlite/jupyterlite",
  "bugs": {
    "url": "https://github.com/jupyterlite/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlite/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js",
    "src/**/*.{ts,tsx}"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "~4.6.0-alpha.1",
    "@jupyterlab/apputils": "~4.7.0-alpha.1",
    "@jupyterlab/coreutils": "~6.6.0-alpha.1",
    "@jupyterlab/nbformat": "~4.6.0-alpha.1",
    "@jupyterlab/observables": "~5.6.0-alpha.1",
    "@jupyterlab/pluginmanager": "~4.6.0-alpha.1",
    "@jupyterlab/services": "~7.6.0-alpha.1",
    "@jupyterlab/settingregistry": "~4.6.0-alpha.1",
    "@jupyterlab/statedb": "~4.6.0-alpha.1",
    "@jupyterlab/translation": "~4.6.0-alpha.1",
    "@jupyterlite/application": "^0.8.0-alpha.0",
    "@jupyterlite/services": "^0.8.0-alpha.0",
    "@jupyterlite/types": "^0.8.0-alpha.0",
    "@lumino/application": "^2.4.5",
    "@lumino/coreutils": "^2.2.2",
    "@lumino/signaling": "^2.1.5",
    "@types/emscripten": "^1.39.6",
    "mock-socket": "^9.3.1"
  },
  "devDependencies": {
    "@types/serviceworker": "^0.0.56",
    "rimraf": "~5.0.1",
    "typescript": "~5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "coreDependency": true
  },
  "styleModule": "style/index.js"
}
