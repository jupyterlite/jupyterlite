{
  "name": "@jupyterlite/theme-extension",
  "version": "0.1.0-alpha.0",
  "description": "JupyterLite - Theme Extension",
  "homepage": "https://github.com/jtpio/jupyterlite",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlite.git"
  },
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0-alpha.10",
    "@jupyterlab/apputils": "^3.1.0-alpha.10",
    "@jupyterlab/coreutils": "^5.1.0-alpha.10",
    "@jupyterlab/settingregistry": "^3.1.0-alpha.10",
    "@jupyterlite/theme": "^0.1.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rimraf": "~3.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~4.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  },
  "styleModule": "style/index.js"
}
