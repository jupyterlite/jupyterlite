{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/application",
    "packages/application-extension",
    "packages/apputils",
    "packages/apputils-extension",
    "packages/localforage",
    "packages/repl-extension",
    "packages/notebook-application-extension",
    "packages/services-extension",
    "packages/types",
    "packages/ui-components"
  ],
  "githubPages": false,
  "logLevel": "Verbose",
  "name": "@jupyterlite",
  "out": "build/typedoc",
  "readme": "none",
  "tsconfig": "tsconfig.typedoc.json",
  "plugin": ["typedoc-plugin-markdown"]
}
